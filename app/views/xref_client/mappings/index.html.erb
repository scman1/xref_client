<p id="notice"><%= notice %></p>

<h1>Mappings</h1>

<table>
  <thead>
    <tr>
      <th>Obj name</th>
      <th>Origin</th>
      <th>Target</th>
      <th>Type</th>
      <th>Default</th>
      <th>Json paths</th>
      <th>Evaluate</th>
      <th>Other</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @mappings.each do |mapping| %>
      <tr>
        <td><%= mapping.obj_name %></td>
        <td><%= mapping.origin %></td>
        <td><%= mapping.target %></td>
        <td><%= mapping.target_type %></td>
        <td><%= mapping.default %></td>
        <td><%= mapping.json_paths %></td>
        <td><%= mapping.evaluate %></td>
        <td><%= mapping.other %></td>
        <td><%= link_to 'Show', mapping %></td>
        <td><%= link_to 'Edit', edit_mapping_path(mapping) %></td>
        <td><%= link_to 'Destroy', mapping, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Mapping', new_mapping_path %>
